{
	"name": "nestql",
	"version": "0.0.1",
	"description": "<p align=\"center\">   <a href=\"http://nestjs.com/\" target=\"blank\"><img src=\"https://nestjs.com/img/logo-small.svg\" width=\"120\" alt=\"Nest Logo\" /></a> </p>",
	"author": "",
	"private": true,
	"license": "UNLICENSED",
	"scripts": {
		"build": "nest build",
		"format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
		"start": "bun src/app/main.ts",
		"start:dev": "bun --watch run src/app/main.ts",
		"start:dev:docker": "bunx concurrently --kill-others \"bunx prisma generate --watch\" \"bun --watch run src/app/main.ts\"",
		"start:debug": "bun --inspect run src/app/main.ts",
		"start:prod": "bun dist/src/app/main.js",
		"mastra:start:dev": "bunx dotenv-cli -e .env -e .env.local -- mastra dev",
		"lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
		"test:all": "bun test",
		"test:watch": "bun test --watch",
		"test:cov": "bun test --coverage",
		"test:debug": "bun --inspect-brk test",
		"test:e2e": "bun test test/**/*.e2e-spec.ts",
		"---------DOCKER---------": "----------------------------------",
		"docker:show:diskSpace": "docker system df",
		"docker:pruneContainers": "docker rm $(docker ps -a -q) || true",
		"docker:pruneImages": "docker image prune -a -f",
		"docker:pruneVolumes": "docker volume prune -a -f",
		"docker:pruneBuildCache": "docker buildx prune -f",
		"docker:pruneNetworks": "docker network prune -f",
		"docker:pruneSystem": "docker system prune --volumes -af",
		"docker:clean": "bun docker:pruneContainers && bun docker:pruneImages && bun docker:pruneVolumes && bun docker:pruneBuildCache && bun docker:pruneNetworks && bun docker:pruneSystem && bun docker:show:diskSpace",
		"---------NQL-DOCKER---------": "----------------------------------",
		"docker:dev:nql-external-services:up": "docker compose --env-file .env -f src/base/docker/nql-external-services/docker-compose.nql-external-services.dev.yaml up",
		"docker:dev:nql-external-services:down": "docker compose --env-file .env -f src/base/docker/nql-external-services/docker-compose.nql-external-services.dev.yaml down --remove-orphans",
		"docker:dev:nql-nginx:up": "docker compose --env-file .env -f src/base/docker/nql-nginx/docker-compose.nql-nginx.dev.yaml up",
		"docker:dev:nql-nginx:down": "docker compose --env-file .env -f src/base/docker/nql-nginx/docker-compose.nql-nginx.dev.yaml down --remove-orphans",
		"docker:dev:nql-external-services:rm:volumes": "docker volume rm nql-external-services_database-development && docker volume rm nql-external-services_redis-development",
		"docker:dev:nql-external-services:clean": "bun docker:dev:nql-external-services:down && bun docker:dev:nql-external-services:rm:volumes",
		"docker:dev:nql-app:up": "docker compose --env-file .env -f src/base/docker/nql-external-services/docker-compose.nql-external-services.dev.yaml -f src/base/docker/nql-app/docker-compose.nql-app.base.yaml -f src/base/docker/nql-app/docker-compose.nql-app.dev.yaml -f src/base/docker/nql-nginx/docker-compose.nql-nginx.dev.yaml up",
		"docker:dev:nql-app:down": "docker compose --env-file .env -f src/base/docker/nql-external-services/docker-compose.nql-external-services.dev.yaml -f src/base/docker/nql-app/docker-compose.nql-app.base.yaml -f src/base/docker/nql-app/docker-compose.nql-app.dev.yaml -f src/base/docker/nql-nginx/docker-compose.nql-nginx.dev.yaml down  --remove-orphans",
		"docker:prod:nql-app:up": "docker compose --env-file .env -f src/base/docker/nql-app/docker-compose.nql-app.base.yaml -f src/base/docker/nql-app/docker-compose.nql-app.prod.yaml up",
		"docker:prod:nql-app:down": "docker compose --env-file .env -f src/base/docker/nql-app/docker-compose.nql-app.base.yaml -f src/base/docker/nql-app/docker-compose.nql-app.prod.yaml down  --remove-orphans",
		"---------PRISMA---------": "----------------------------------",
		"prisma:generate": "bunx prisma generate",
		"prisma:generate:watch": "bunx prisma generate --watch",
		"prisma:seed": "bun prisma/seed.ts",
		"prisma:migrate:reset": "bunx prisma migrate reset --force",
		"prisma:migrate:dev": "bunx prisma migrate dev",
		"prisma:migrate:dev:name": "bunx prisma migrate dev --name",
		"prisma:migrate:dev:reset:full": "rm -rf prisma/migrations && bun prisma:migrate:reset && bun prisma:migrate:dev:name init && bun prisma:seed",
		"----PRISMA-DOCKER----": "----------------------------------",
		"prisma:generate:docker": "docker exec -it nql-app-development bash -c 'bunx prisma generate' && reset",
		"prisma:seed:docker": "docker exec -it nql-app-development bash -c 'bun prisma/seed.ts' && reset",
		"prisma:migrate:reset:docker": "docker exec -it nql-app-development bash -c 'rm -rf prisma/migrations && bunx prisma migrate reset --force' && reset",
		"prisma:migrate:dev:docker": "docker exec -it nql-app-development bash -c 'bunx prisma migrate dev' && reset",
		"prisma:migrate:dev:name:docker": "src/base/sh-scripts/prisma-migrate-name.sh",
		"prisma:migrate:dev:reset:full:docker": "docker exec -it nql-app-development bash -c 'rm -rf prisma/migrations && bunx prisma migrate reset --force && bunx prisma migrate dev --name init && bun prisma/seed.ts' && reset",
		"---------NQL---------": "-----------------------------------",
		"nql:dev:graphql:url": "echo 'https://local.nestql.com/graphql'",
		"nql:gen:password": "bun -e \"import { randomBytes } from 'crypto'; console.log(randomBytes(12).toString('base64').replace(/[^a-zA-Z0-9]/g, '').slice(0, 16))\"",
		"nql:gen:session-secret": "bun -e \"import { randomBytes } from 'crypto'; console.log(randomBytes(24).toString('hex'))\"",
		"nql:dev:app:generate-certs": "sudo src/base/sh-scripts/generate-certs.sh && sudo src/base/sh-scripts/update-hosts.sh",
		"nql:dev:app:update-hosts": "sudo src/base/sh-scripts/update-hosts.sh",
		"nql:dev:app:setup": "bun nql:dev:app:generate-certs && bun nql:dev:app:update-hosts",
		"nql:dev:app:docker:shell": "docker exec -it nql-app-development bash",
		"nql:generate:model": "bunx prisma generate && bun src/base/generators/model-generator/start.ts"
	},
	"dependencies": {
		"@ai-sdk/amazon-bedrock": "2.2.12",
		"@ai-sdk/openai": "1.3.23",
		"@ai-sdk/provider": "1.1.3",
		"@mastra/core": "0.11.1",
		"@mastra/loggers": "0.10.4",
		"@mastra/mcp": "0.10.7",
		"@mastra/memory": "0.11.5",
		"@mastra/pg": "0.12.5",
		"@nestjs/apollo": "13.1.0",
		"@nestjs/common": "11.1.5",
		"@nestjs/config": "4.0.2",
		"@nestjs/core": "11.1.5",
		"@nestjs/graphql": "13.1.0",
		"@nestjs/platform-express": "11.1.5",
		"@prisma/client": "6.12.0",
		"class-transformer": "0.5.1",
		"class-validator": "0.14.2",
		"connect-redis": "9.0.0",
		"dotenv": "17.2.1",
		"express-session": "1.18.2",
		"graphql": "16.11.0",
		"graphql-type-json": "0.3.2",
		"mastra": "0.10.15",
		"pluralize": "8.0.0",
		"prisma": "6.12.0",
		"prisma-nestjs-graphql": "21.2.0",
		"redis": "5.6.1",
		"reflect-metadata": "0.2.2",
		"rxjs": "7.8.2",
		"uuid": "11.1.0",
		"zod": "3.25.76"
	},
	"devDependencies": {
		"@faker-js/faker": "9.9.0",
		"@nestjs/cli": "11.0.7",
		"@nestjs/schematics": "11.0.5",
		"@nestjs/testing": "11.1.5",
		"@prisma/internals": "6.12.0",
		"@types/express": "5.0.3",
		"@types/jscodeshift": "17.3.0",
		"@types/node": "24.1.0",
		"@typescript-eslint/eslint-plugin": "8.38.0",
		"@typescript-eslint/parser": "8.38.0",
		"bun-types": "1.2.19",
		"change-case": "5.4.4",
		"concurrently": "9.2.0",
		"dotenv-cli": "9.0.0",
		"eslint": "9.32.0",
		"eslint-config-prettier": "10.1.8",
		"eslint-plugin-prettier": "5.5.3",
		"jscodeshift": "17.3.0",
		"open": "10.2.0",
		"prettier": "3.6.2",
		"source-map-support": "0.5.21",
		"tsconfig-paths": "4.2.0",
		"typescript": "5.8.3"
	},
	"type": "module",
	"main": "src/main.ts",
	"directories": {
		"test": "test"
	},
	"keywords": [],
	"trustedDependencies": [
		"@apollo/protobufjs",
		"@nestjs/core",
		"@prisma/client",
		"@prisma/engines",
		"esbuild",
		"prisma",
		"protobufjs"
	]
}
