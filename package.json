{
	"name": "nestql",
	"version": "0.0.1",
	"description": "<p align=\"center\">   <a href=\"http://nestjs.com/\" target=\"blank\"><img src=\"https://nestjs.com/img/logo-small.svg\" width=\"120\" alt=\"Nest Logo\" /></a> </p>",
	"author": "",
	"private": true,
	"license": "UNLICENSED",
	"scripts": {
		"build": "nest build",
		"format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
		"start": "bun run src/app/main.ts",
		"start:dev": "bun --watch run src/app/main.ts",
		"start:debug": "bun --inspect run src/app/main.ts",
		"start:prod": "bun run dist/app/main.js",
		"lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
		"test": "bun test",
		"test:watch": "bun test --watch",
		"test:cov": "bun test --coverage",
		"test:debug": "bun --inspect-brk test",
		"test:e2e": "bun test test/**/*.e2e-spec.ts",
		"nql:docker:show:diskSpace": "docker system df",
		"nql:docker:pruneContainers": "docker rm $(docker ps -a -q) || true",
		"nql:docker:pruneImages": "docker image prune -a -f",
		"nql:docker:pruneVolumes": "docker volume prune -a -f",
		"nql:docker:pruneBuildCache": "docker buildx prune -f",
		"nql:docker:pruneNetworks": "docker network prune -f",
		"nql:docker:pruneSystem": "docker system prune --volumes -af",
		"nql:docker:clean": "bun run nql:docker:pruneContainers && bun run nql:docker:pruneImages && bun run nql:docker:pruneVolumes && bun run nql:docker:pruneBuildCache && bun run nql:docker:pruneNetworks && bun run nql:docker:pruneSystem && bun run nql:docker:show:diskSpace",
		"nql:dev:docker:addCerts": "sudo src/base/sh-scripts/generate-certs.sh && sudo src/base/sh-scripts/update-hosts.sh",
		"nql:dev:docker:externalServices:up": "docker compose --env-file .env -f src/base/docker/external-services/docker-compose.dev.external-services.yaml up",
		"nql:dev:docker:externalServices:down": "docker compose --env-file .env -f src/base/docker/external-services/docker-compose.dev.external-services.yaml down --remove-orphans",
		"nql:dev:prisma:generate": "bun prisma generate",
		"nql:dev:prisma:generate:watch": "bun prisma generate --watch",
		"nql:dev:prisma:migrate": "dotenv -e .env -- bun prisma migrate dev",
		"nql:dev:prisma:migrate:name": "dotenv -e .env -- bun prisma migrate dev --name",
		"nql:dev:prisma:reset": "dotenv -e .env -- bun prisma migrate reset --force",
		"nql:dev:prisma:seed": "dotenv -e .env -- bun run prisma/seed.ts",
		"nql:dev:prisma:reset:full": "rm -rf prisma/migrations && bun prisma:dev:reset && bun prisma:dev:migrate && bun prisma:dev:seed"
	},
	"dependencies": {
		"@nestjs/apollo": "^12.2.1",
		"@nestjs/common": "^10.0.0",
		"@nestjs/config": "^3.3.0",
		"@nestjs/core": "^10.0.0",
		"@nestjs/graphql": "^12.2.1",
		"@nestjs/platform-express": "^10.0.0",
		"@prisma/client": "^6.0.0",
		"class-transformer": "^0.5.1",
		"class-validator": "^0.14.1",
		"connect-redis": "^8.0.0",
		"dotenv": "^16.4.5",
		"express-session": "^1.18.1",
		"graphql": "^16.9.0",
		"graphql-type-json": "^0.3.2",
		"pluralize": "^8.0.0",
		"prisma": "^6.0.0",
		"prisma-nestjs-graphql": "^20.0.3",
		"redis": "^4.7.0",
		"reflect-metadata": "^0.2.0",
		"rxjs": "^7.8.1",
		"uuid": "^11.0.3"
	},
	"devDependencies": {
		"@faker-js/faker": "^9.2.0",
		"@nestjs/cli": "^10.0.0",
		"@nestjs/schematics": "^10.0.0",
		"@nestjs/testing": "^10.0.0",
		"@types/express": "^5.0.0",
		"@types/node": "^20.3.1",
		"@types/supertest": "^6.0.0",
		"@typescript-eslint/eslint-plugin": "^8.0.0",
		"@typescript-eslint/parser": "^8.0.0",
		"bun-types": "latest",
		"dotenv-cli": "^7.4.4",
		"eslint": "^8.0.0",
		"eslint-config-prettier": "^9.0.0",
		"eslint-plugin-prettier": "^5.0.0",
		"prettier": "^3.0.0",
		"source-map-support": "^0.5.21",
		"supertest": "^7.0.0",
		"tsconfig-paths": "^4.2.0",
		"typescript": "^5.1.3"
	},
	"type": "module",
	"main": "src/main.ts",
	"directories": {
		"test": "test"
	},
	"keywords": []
}
