{
	"name": "nestql",
	"version": "0.0.1",
	"description": "<p align=\"center\">   <a href=\"http://nestjs.com/\" target=\"blank\"><img src=\"https://nestjs.com/img/logo-small.svg\" width=\"120\" alt=\"Nest Logo\" /></a> </p>",
	"author": "",
	"private": true,
	"license": "UNLICENSED",
	"scripts": {
		"build": "nest build",
		"format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
		"start": "bun src/app/main.ts",
		"start:dev": "bun --watch run src/app/main.ts",
		"start:dev:docker": "bunx concurrently --kill-others \"bunx prisma generate --watch\" \"bun --watch run src/app/main.ts\"",
		"start:debug": "bun --inspect run src/app/main.ts",
		"start:prod": "bun dist/app/main.js",
		"lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
		"test": "bun test",
		"test:watch": "bun test --watch",
		"test:cov": "bun test --coverage",
		"test:debug": "bun --inspect-brk test",
		"test:e2e": "bun test test/**/*.e2e-spec.ts",
		"---------DOCKER---------": "----------------------------------",
		"docker:nql-app:up": "docker compose --env-file .env -f src/base/docker/nql-app/docker-compose.nql-app.yaml up",
		"docker:nql-app:down": "docker compose --env-file .env -f src/base/docker/nql-app/docker-compose.nql-app.yaml down --remove-orphans",
		"docker:nql-external-services:up": "docker compose --env-file .env -f src/base/docker/nql-external-services/docker-compose.dev.nql-external-services.yaml up",
		"docker:nql-external-services:down": "docker compose --env-file .env -f src/base/docker/nql-external-services/docker-compose.dev.nql-external-services.yaml down --remove-orphans",
		"docker:nql-external-services:rm:volumes": "docker volume rm nql-external-services_database-development && docker volume rm nql-external-services_redis-development",
		"docker:nql-external-services:clean": "bun docker:nql-external-services:down && bun docker:nql-external-services:rm:volumes",
		"docker:show:diskSpace": "docker system df",
		"docker:pruneContainers": "docker rm $(docker ps -a -q) || true",
		"docker:pruneImages": "docker image prune -a -f",
		"docker:pruneVolumes": "docker volume prune -a -f",
		"docker:pruneBuildCache": "docker buildx prune -f",
		"docker:pruneNetworks": "docker network prune -f",
		"docker:pruneSystem": "docker system prune --volumes -af",
		"docker:clean": "bun docker:pruneContainers && bun docker:pruneImages && bun docker:pruneVolumes && bun docker:pruneBuildCache && bun docker:pruneNetworks && bun docker:pruneSystem && bun docker:show:diskSpace",
		"---------PRISMA---------": "----------------------------------",
		"prisma:generate": "bunx prisma generate",
		"prisma:generate:watch": "bunx prisma generate --watch",
		"prisma:seed": "bun prisma/seed.ts",
		"prisma:migrate:reset": "bunx prisma migrate reset --force",
		"prisma:migrate:dev": "bunx prisma migrate dev",
		"prisma:migrate:dev:name": "bunx prisma migrate dev --name",
		"prisma:migrate:dev:reset:full": "rm -rf prisma/migrations && bun prisma:migrate:reset && bun prisma:migrate:dev:name init && bun prisma:seed",
		"----PRISMA-DOCKER----": "----------------------------------",
		"prisma:generate:docker": "docker exec -it nql-app-development bash -c 'bunx prisma generate'",
		"prisma:migrate:dev:docker": "docker exec -it nql-app-development bash -c 'bunx prisma migrate dev'",
		"prisma:migrate:dev:name:docker": "docker exec -it nql-app-development /bin/bash -c 'bunx prisma migrate dev --name $1' -- ",
		"prisma:migrate:dev:reset:full:docker": "docker exec -it nql-app-development bash -c 'rm -rf prisma/migrations && bunx prisma migrate reset --force && bunx prisma migrate dev --name init && bun prisma/seed.ts'",
		"---------NQL---------": "-----------------------------------",
		"nql:dev:graphql:url": "echo 'https://local.nestql.com/graphql'",
		"nql:dev:app:generate-certs": "sudo src/base/sh-scripts/generate-certs.sh && sudo src/base/sh-scripts/update-hosts.sh",
		"nql:dev:app:update-hosts": "sudo src/base/sh-scripts/update-hosts.sh",
		"nql:dev:app:setup": "bun nql:dev:app:generate-certs && bun nql:dev:app:update-hosts"
	},
	"dependencies": {
		"@nestjs/apollo": "12.2.1",
		"@nestjs/common": "10.0.0",
		"@nestjs/config": "3.3.0",
		"@nestjs/core": "10.0.0",
		"@nestjs/graphql": "12.2.1",
		"@nestjs/platform-express": "10.0.0",
		"@prisma/client": "6.0.1",
		"class-transformer": "0.5.1",
		"class-validator": "0.14.1",
		"connect-redis": "8.0.0",
		"dotenv": "16.4.5",
		"express-session": "1.18.1",
		"graphql": "16.9.0",
		"graphql-type-json": "0.3.2",
		"pluralize": "8.0.0",
		"prisma": "6.0.1",
		"prisma-nestjs-graphql": "20.0.3",
		"redis": "4.7.0",
		"reflect-metadata": "0.2.0",
		"rxjs": "7.8.1",
		"uuid": "11.0.3"
	},
	"devDependencies": {
		"@faker-js/faker": "9.2.0",
		"@nestjs/cli": "10.0.0",
		"@nestjs/schematics": "10.0.0",
		"@nestjs/testing": "10.0.0",
		"@types/express": "5.0.0",
		"@types/node": "20.3.1",
		"@typescript-eslint/eslint-plugin": "8.0.0",
		"@typescript-eslint/parser": "8.0.0",
		"bun-types": "latest",
		"concurrently": "9.1.0",
		"dotenv-cli": "7.4.4",
		"eslint": "8.0.0",
		"eslint-config-prettier": "9.0.0",
		"eslint-plugin-prettier": "5.0.0",
		"open": "10.1.0",
		"prettier": "3.0.0",
		"source-map-support": "0.5.21",
		"tsconfig-paths": "4.2.0",
		"typescript": "5.1.3"
	},
	"type": "module",
	"main": "src/main.ts",
	"directories": {
		"test": "test"
	},
	"keywords": []
}
